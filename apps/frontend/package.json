{
  "name": "@wfp-dmp/frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "next build",
    "build:analyze": "DEV_ANALYZE_BUNDLE=true next build",
    "dev": "./gen_env.sh local public && next dev -p 3000",
    "lint": "eslint --cache --cache-location .next/cache/eslint/ './!(out)/**/*.{js,ts,tsx}'",
    "lint:fix": "pnpm lint --fix",
    "lint:style": "stylelint './**/*.module.css'",
    "lint:style:fix": "stylelint --fix './**/*.module.css'",
    "postinstall": "test -f .env.local || (test -f .env.sample && cp .env.sample .env.local || echo -e \"\\e[33mNo env file initialized\\e[0m\")",
    "serve:export": "pnpm build && cd out && cp index.html 200.html && npx serve -p 3000",
    "serve:export:local": "./gen_env.sh local public && pnpm build && cd out && cp index.html 200.html && npx serve -p 3000",
    "start": "next start",
    "test": "jest",
    "test:circular": "pnpm depcruise --validate .dependency-cruiser.js pages --ignore-known",
    "test:coverage": "pnpm test -- --coverage",
    "test:type": "pnpm build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@formatjs/intl-datetimeformat": "^6.18.0",
    "@formatjs/intl-numberformat": "^8.15.4",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@mui/x-data-grid": "^7.10.0",
    "@mui/x-date-pickers": "^7.11.0",
    "@wfp-dmp/interfaces": "workspace:*",
    "axios": "^1.13.5",
    "axios-auth-refresh": "3.3.6",
    "chart.js": "^4.4.3",
    "dayjs": "^1.11.7",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "next": "^15.5.9",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-intl": "7.1.11",
    "react-to-print": "^2.14.12",
    "swr": "^2.1.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "@axe-core/react": "4.4.4",
    "@next/bundle-analyzer": "^15.0.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.0.3",
    "@types/jest-axe": "3.5.4",
    "@types/lodash": "4.14.185",
    "@types/node": "^22.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "dependency-cruiser": "11.15.0",
    "dependency-cruiser-config-custom": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "jest": "29.0.3",
    "jest-axe": "6.0.0",
    "jest-environment-jsdom": "29.0.3",
    "prettier": "2.7.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "28.0.0",
    "stylelint-declaration-strict-value": "1.9.0",
    "ts-node": "^10.9.2",
    "tsconfig": "workspace:*",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": "^20.19.15"
  }
}
